import pandas as pd

df1 = pd.read_csv("Contractors.csv")
df2 = pd.read_csv("user_details_report_1.csv")

df1["Email"]
df2["Email"]

df = pd.concat([df1, df2])

dups_dropped = df.drop_duplicates(df.columns.difference(['Email']), keep=False)
dups_dropped.to_csv('update.csv', index=False)

